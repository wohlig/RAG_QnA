{
  "name": "framework",
  "version": "0.0.1",
  "description": "framework",
  "scripts": {
    "start": "node server.js",
    "develop": "nodemon server.js",
    "debug": "nodemon --inspect=9231 server.js",
    "standard": "standard --fix",
    "middlewareList": "node fetchListForMiddleware.js"
  },
  "nodemonConfig": {
    "env": {
      "NODE_ENV": "development"
    },
    "watch": [
      "controllers",
      "routes"
    ],
    "events": {
      "restart": "user -k 3005/tcp;"
    },
    "delay": "1500"
  },
  "pre-commit": [
    "standard"
  ],
  "standard": {
    "ignore": [
      "/lib/"
    ]
  },
  "devDependencies": {
    "jsdoc": "^3.6.6",
    "jsdoc-http-plugin": "^0.3.2",
    "nodemon": "^2.0.2",
    "pre-commit": "^1.2.2",
    "standard": "^14.3.4"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.5.0",
    "@google-cloud/vertexai": "^1.2.0",
    "@google/generative-ai": "^0.13.0",
    "@langchain/anthropic": "^0.2.1",
    "@langchain/community": "^0.2.5",
    "@langchain/google-genai": "^0.0.20",
    "@langchain/google-vertexai": "^0.0.18",
    "@langchain/google-vertexai-web": "^0.0.19",
    "@langchain/mongodb": "^0.0.4",
    "@pinecone-database/pinecone": "^2.2.2",
    "axios": "^1.2.1",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "elastic-apm-node": "^3.41.0",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "helmet": "^6.0.1",
    "html-to-text": "^9.0.5",
    "jsdom": "^24.1.0",
    "jsonschema": "^1.4.1",
    "jsonwebtoken": "^9.0.0",
    "langchain": "^0.2.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongodb": "^6.7.0",
    "mongoose": "^6.8.1",
    "mongoose-timestamp-plugin": "^0.0.1",
    "multer": "^1.4.5-lts.1",
    "node-vault": "^0.9.22",
    "openai": "^4.0.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pdf-parse": "^1.1.1",
    "pdfreader": "^3.0.2",
    "puppeteer": "^19.11.1",
    "redis": "^3.0.2",
    "uuidv4": "^6.2.13",
    "zod": "^3.23.8"
  }
}
